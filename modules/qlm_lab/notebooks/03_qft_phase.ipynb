   "source": [
    "from qlm_lab.tools import quantum_np as Q\n",
    "import numpy as np\n",
    "n=3\n",
    "N=2**n\n",
    "freq=3\n",
    "amps = np.array([np.exp(2j*np.pi*freq*j/N)/np.sqrt(N) for j in range(N)], dtype=complex)\n",
    "np.abs(Q.apply_qft(amps))**2"
   ]
  }
