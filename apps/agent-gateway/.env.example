# Agent Gateway Configuration
AGENT_GATEWAY_PORT=3001

# Prism Shell Path (optional, defaults to ../../prism/prismsh.js)
PRISM_SHELL_PATH=/path/to/prism/prismsh.js

# Agent Directory Path (optional, defaults to ../../agents)
AGENTS_PATH=/path/to/agents

# Logging
LOG_LEVEL=info

# CORS
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
