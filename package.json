{
  "name": "blackroad-prism-console",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo workspace for the BlackRoad Prism Console front-end and service demos.",
  "type": "commonjs",
  "main": "server_full.js",
  "engines": {
    "node": ">=20"
  },
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "start": "node server_full.js",
    "dev": "nodemon server_full.js",
    "build": "next build",
    "seed": "node scripts/seed_admin.js",
    "health": "node scripts/health.js",
    "test": "jest",
    "test:api": "node tests/smoke.test.mjs",
    "test:db": "node tests/test_db.js",
    "test:auth": "node tests/test_auth.js",
    "test:projects": "node tests/test_projects_tasks.js",
    "test:logs": "node tests/test_logs_contradictions.js",
    "test:jest": "jest --runInBand",
    "test:compliance": "vitest run",
    "test-agents-pack": "node tests/test_agents_mega.js",
    "lint": "eslint . --ext .js,.jsx,.mjs,.cjs || true",
    "format": "prettier -w .",
    "format:check": "prettier --check .",
    "typecheck": "tsc -p tsconfig.json || echo 'no typecheck'",
    "prepare": "husky install",
    "guard:dev-mode": "node scripts/security/dev_mode_guard.js",
    "branch-cleanup:dry": "tsx tools/branch-cleanup/cleanup.ts --dry-run",
    "branch-cleanup:run": "tsx tools/branch-cleanup/cleanup.ts",
    "frontend:dev": "npm --prefix frontend run dev",
    "frontend:build": "npm --prefix frontend run build",
    "frontend:test": "npm --prefix frontend run test",
    "frontend:preview": "npm --prefix frontend run preview",
    "site:dev": "npm --prefix sites/blackroad run dev",
    "site:build": "npm --prefix sites/blackroad run build",
    "next:start": "next start",
    "next:dev": "next dev",
    "next:build": "next build",
    "fix-anything": "node .github/tools/codex-apply.js .github/prompts/codex-fix-anything.md || true",
    "postinstall": "npm install --prefix frontend || true"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.632.0",
    "@aws-sdk/client-ssm": "^3.614.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.4.3",
    "bs58": "^6.0.0",
    "commander": "^12.1.0",
    "compression": "^1.8.1",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ethers": "^6.10.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.3.0",
    "graphql": "^16.9.0",
    "helmet": "^7.1.0",
    "ipfs-http-client": "^60.0.1",
    "json-canonicalize": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.460.0",
    "minimatch": "^9.0.5",
    "morgan": "^1.10.0",
    "next": "^15.1.0",
    "pdf-parse": "^1.1.1",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io": "^4.7.5",
    "sqlite3": "^5.1.7",
    "stripe": "^12.18.0",
    "systeminformation": "^5.25.11",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "yaml": "^2.6.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@octokit/core": "^6.1.2",
    "@octokit/plugin-paginate-graphql": "^5.2.4",
    "@octokit/plugin-throttling": "^9.3.2",
    "@types/node": "^22.9.0",
    "@types/node-fetch": "^2.6.11",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.20",
    "@typescript-eslint/parser": "^8.11.0",
    "buffer-from": "^1.1.2",
    "chalk": "^5.6.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.15.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "json2csv": "^6.0.0-alpha.2",
    "lint-staged": "^16.1.5",
    "nodemon": "^3.0.2",
    "p-limit": "^6.1.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "supertest": "^6.3.4",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^1.6.0",
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "*.{js,jsx,mjs,cjs,ts,tsx}": [
      "eslint --fix",
      "prettier -w"
    ],
    "*": "prettier -w --ignore-unknown"
  }
}
