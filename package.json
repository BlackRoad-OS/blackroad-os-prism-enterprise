{
  "name": "blackroad-prism-console",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo workspace for the BlackRoad Prism Console front-end.",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "npm --prefix frontend run dev",
    "build": "npm --prefix frontend run build",
    "preview": "npm --prefix frontend run preview",
    "test": "npm --prefix frontend run test",
    "branch-cleanup:dry": "tsx tools/branch-cleanup/cleanup.ts --dry-run",
    "branch-cleanup:run": "tsx tools/branch-cleanup/cleanup.ts"
  },
  "devDependencies": {
    "@octokit/core": "^6.1.2",
    "@octokit/plugin-paginate-graphql": "^5.2.4",
    "@octokit/plugin-throttling": "^9.3.2",
    "@types/node": "^22.9.0",
    "dayjs": "^1.11.13",
    "json2csv": "^6.0.0-alpha.2",
    "p-limit": "^6.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "yaml": "^2.6.1"
    "test": "npm --prefix frontend run test"
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.5",
    "systeminformation": "^5.25.11",
    "uuid": "^9.0.1",
    "better-sqlite3": "^9.4.3",
    "stripe": "^12.18.0",
    "ethers": "^6.10.0",
    "bs58": "^6.0.0",
    "json-canonicalize": "^2.0.0",
    "ipfs-http-client": "^60.0.1"
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@typescript-eslint/parser": "^6.21.0"
    "@eslint/js": "^9.12.0",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "@eslint/js": "^9.12.0",
    "prettier": "^3.3.3",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.7"
  },
  "lint-staged": {
    "*.{js,jsx,mjs,cjs,ts,tsx}": [
      "eslint --fix",
      "prettier -w"
    ],
    "*": "prettier -w --ignore-unknown"
    "@eslint/js": "^9.11.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.0.2"
  }
}
