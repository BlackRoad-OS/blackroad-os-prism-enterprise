{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "types": [
      "next",
      "next/types/global",
      "react",
      "react-dom"
    ],
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
